{
  "name": "hello-playwright-typescript",
  "version": "1.0.0",
  "description": "Playwright TypeScript starter project",
  "type": "module",
  "scripts": {
    "clean": "npm run rmdir dist node_modules",
    "cleandep": "npm run rmdir node_modules",
    "lint": "eslint .",
    "rmdir": "node -e \"var fs = require('fs'); try{process.argv.slice(1).map((fpath) => fs.rmdirSync(fpath, { recursive: true }))}catch(err){console.log(err)}; process.exit(0);\"",
    "build": "tsc -b -v --listFiles",
    "start": "node dist/src/main.js",
    "dev": "npm run build && npm run start"
  },
  "author": "",
  "license": "Unlicense",
  "dependencies": {
    "commander": "^13.1.0",
    "playwright": "^1.52.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^22.12.0",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.48.0"
  }
}
